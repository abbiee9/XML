<?xml version="1.0" encoding="UTF-8"?>
<at:schema xmlns:at="http://www.w3.org/2001/XMLSchema" >

<ap:element name="book">
	<ap:complexType>
	  <ap:sequence>
	    <ap:element name="name" type="ap:string"/>
	    
	   
	   
	    
	    <ap:element name="description" type="ap:string"/>
	    <ap:element name="calories" type="ap:string"/>
	    
	     
	    
	    <ap:element name="deliverystatus" type="ap:boolean"/>
	  </ap:sequence>
	</ap:complexType>

</ap:element> 

<ap:element name="Library">
	<ap:complexType>
	  <ap:sequence>
	   <ap:element ref="book" maxOccurs="20"></ap:element>
	  </ap:sequence>
	</ap:complexType>

</ap:element> 


</at:schema>